<!DOCTYPE html>
<html>
<head>
  <title><%= t 'misc.site_title' %></title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
<% flash.each do |name, msg| %>
  <%= content_tag :div, msg, :id => "flash_#{name}" %>
<% end %>
<%= render :partial => 'layouts/app_header' %>
<%= yield  %>
</body>
</html>
